@model List<WhatsIn.Models.RatingViewModel>
<div>
    @{
        for (int i = 0; i < Model.Count(); i++)
        {
            var item = Model[i];
            <div class="row">                
                <div class="col-xs-4 text-center">
                    <span style="font-weight:bold; text-align:left;">@item.LoginName </span><br /><br />
                    <span style="text-align:left;color:red;"> @item.Comment </span><br /><br />
                </div>
            </div>
        }
    }
</div>
