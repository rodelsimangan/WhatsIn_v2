@model List<WhatsIn.Models.ServiceModel>
<div>
    @for (int i = 0; i < Model.Count(); i++)
    {
        var item = Model[i];


        @:<div class="row">
              <div class="col-xs-12 text-center">
                  <h4><a href="#" id="@item.Id" class="UpdateServiceButton">@item.Name </a></h4>

                  <button id="@item.Id" class="btn btn-circle btn-xs red AddServiceImageButton">
                      <img id="@item.Id" class="img-thumbnail imgService" alt="Add Image" />
                  </button><br />
                  <button data-toggle="modal" id="@item.Id" class="btn btn-success RemoveServiceButton"><span class="fa fa-trash"></span>&nbsp; Remove</button><br /><br /><br />
              </div>
       @:</div><div class="row">&nbsp;</div>


    }
</div>