<div class="modal fade" id="modalTermsAndCon" role="dialog" >
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>        
      </div>
      <div class="modal-body">
		<strong>Warning!</strong> By clicking the button "I Agree!" you are agreeing that you have read and accepted our Terms of Use, Disclaimer and Privacy Policy. Please review our <a href="@(Url.Action("TermsOfUse", "Home"))" style="color:rgb(208, 88, 3);">Terms of Use</a>, <a href="@(Url.Action("Disclaimer", "Home"))" style="color:rgb(208, 88, 3);">Disclaimer</a>, and <a href="@(Url.Action("PrivacyPolicy", "Home"))" style="color:rgb(208, 88, 3);">Privacy Policy</a> for more details.
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <button type="button" id="btnIAgree" class="btn btn-primary">I Agree!</button>
      </div>
    </div>
  </div>
</div>

</html>
<script>

$('#btnIAgree').click(function (e){	
	e.preventDefault();
	window.location.href = "@Url.Action("UpdateIsServiceExistsTag", "Home")";
});

$(function () {

    $(".divItineraryBadge").each(function (e) {
        var loginId = $(this).prop('id');
         $.ajax({
            type: "Get",
            url: "@Url.Action("MyItineraryCount", "Home")",
            data: {
                loginId: loginId,
            },
            success: function (data) {
                $('.divItineraryBadge').html(data);
            }
        });
      });


    $(".divNameOfUser").each(function (e) {
        var loginId = $(this).prop('id');
         $.ajax({
            type: "Get",
            url: "@Url.Action("GetNameOfUser", "Home")",
            data: {
                loginId: loginId,
            },
            success: function (data) {
                $('.divNameOfUser').html(data);
            }
        });
      });
});

</script>