
@model WhatsIn.Models.ApplicationUser


    <div class="row col-md-12">
        <img src="@Model.ProfilePicture" class="img-thumbnail" style="width:50%;height:50%;"  alt="@Model.Name" />
    </div>
    <div class="row col-md-12">
        @Model.Name<br />
    </div>
    @if (!String.IsNullOrEmpty(Model.WebsiteAddress))
    {
        <div class="row col-md-12">
            <a href="@Model.WebsiteAddress" target="_blank">@Model.WebsiteAddress</a>
        </div>
    }



